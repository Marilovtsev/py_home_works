def send_messages(printed_messages, sent_messages):
    while printed_messages:
        current_messages = printed_messages.pop()
        print(f"Output messages: {current_messages}")
        sent_messages.append(current_messages)


printed_messages = ['Mass Effect 1', 'Mass Effect 2', 'Mass Effect 3']
sent_messages = []
send_messages(printed_messages[:], sent_messages)
print(f"printed_messages: {printed_messages}, sent_messages: {sent_messages}")
